{
  "name": "ryzhkov.ro.app",
  "version": "0.2.3",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "build.android": "npm run build && npx cap sync android",
    "build.ios": "npm run build && npx cap sync ios",
    "build.icons": "capacitor-assets generate",
    "deploy.prepare": "npm run && npm run version:update && npm run version:commit",
    "deploy.android": "npm run build.android && npm run deploy.prepare && npx cap open android",
    "deploy.ios": "npm run build.ios && npm run deploy.prepare && npx cap open ios",
    "dev": "vite",
    "format": "prettier .  --write --fix",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore --fix src",
    "lint:style": "stylelint src/**/*.{css,scss,vue}",
    "version:update": "sh bin/update-version.sh",
    "version:commit": "git add ./.build ./android ./ios ./package.json && git commit -m \"Increment build number\" && git push"
  },
  "dependencies": {
    "@awesome-cordova-plugins/calendar": "^6.4.0",
    "@awesome-cordova-plugins/core": "^6.4.0",
    "@capacitor/android": "^5.7.2",
    "@capacitor/app": "^5.0.7",
    "@capacitor/camera": "^5.0.9",
    "@capacitor/core": "^5.7.2",
    "@capacitor/device": "^5.0.7",
    "@capacitor/dialog": "^5.0.7",
    "@capacitor/filesystem": "^5.2.1",
    "@capacitor/geolocation": "^5.0.7",
    "@capacitor/ios": "^5.7.2",
    "@capacitor/preferences": "^5.0.7",
    "@capacitor/splash-screen": "^5.0.7",
    "@capacitor/status-bar": "^5.0.7",
    "@capawesome/capacitor-file-picker": "^5.3.0",
    "@formkit/auto-animate": "^0.8.1",
    "@ionic/pwa-elements": "^3.2.2",
    "@vuepic/vue-datepicker": "^7.2.2",
    "@vueuse/core": "^10.6.1",
    "axios": "^1.6.7",
    "capacitor-appmetrica-plugin": "^3.0.1",
    "capacitor-native-settings": "^5.0.1",
    "capacitor-voice-recorder": "^5.0.0",
    "class-transformer": "^0.5.1",
    "cordova-plugin-calendar": "^5.1.6",
    "dayjs": "^1.11.10",
    "eruda": "^3.0.1",
    "firebase": "^10.8.1",
    "image-blob-reduce": "^4.1.0",
    "openmeteo": "^1.1.3",
    "pinia": "^2.1.7",
    "query-string": "^8.1.0",
    "vue": "^3.4.21",
    "vue-router": "^4.2.5",
    "vue-tel-input": "^8.1.4",
    "vue-use-scroll-size-observer": "^1.0.4",
    "vue-yandex-maps": "^2.0.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-optional-chaining": "^7.22.5",
    "@capacitor/assets": "^3.0.4",
    "@capacitor/cli": "^5.7.2",
    "@capacitor/core": "^5.7.2",
    "@types/node": "^20.11.26",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.16",
    "capacitor-set-version": "^2.2.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "eslint-plugin-vue": "9.19.2",
    "postcss": "^8.4.35",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "sass": "^1.71.1",
    "sass-loader": "14.1.1",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2",
    "vite": "^5.1.6",
    "vue-tsc": "^2.0.6"
  }
}
