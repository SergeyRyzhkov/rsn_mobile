{
  "name": "ryzhkov-customer-app",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "build.android": "npm run build && npx cap sync android",
    "build.ios": "npm run build && npx cap sync ios",
    "deploy.prepare": "npm run version:update && npm run version:commit",
    "deploy.android": "npm run build.android && npm run deploy.prepare &&  npx cap open android",
    "deploy.ios": "npm run build.ios && npm run deploy.prepare && npx cap open ios",
    "dev": "vite",
    "lintfix": "eslint --ext js,ts,tsx,vue src --fix && prettier --write \"src/**/*.{ts,tsx,vue}\"",
    "version:update": "sh bin/update-version.sh",
    "version:commit": "git add ./.build ./android ./ios ./package.json && git commit -m \"Increment build number\" && git push"
  },
  "dependencies": {
    "@awesome-cordova-plugins/calendar": "^6.4.0",
    "@awesome-cordova-plugins/core": "^6.4.0",
    "@capacitor/device": "^5.0.6",
    "@capacitor/splash-screen": "^5.0.6",
    "@formkit/auto-animate": "^0.8.1",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.3",
    "@vuepic/vue-datepicker": "^7.2.2",
    "@vueuse/core": "^10.6.1",
    "@vueuse/integrations": "^10.6.1",
    "axios": "^1.6.2",
    "class-transformer": "^0.5.1",
    "cordova-plugin-calendar": "^5.1.6",
    "dayjs": "^1.11.10",
    "hammerjs": "^2.0.8",
    "image-blob-reduce": "^4.1.0",
    "pinia": "^2.1.7",
    "query-string": "^8.1.0",
    "swiper": "^8.3.2",
    "universal-cookie": "^6.1.1",
    "vue": "^3.3.10",
    "vue-advanced-cropper": "^2.8.3",
    "vue-router": "^4.2.5",
    "vue-tel-input": "^8.1.4",
    "vue-yandex-maps": "^0.12.7"
  },
  "devDependencies": {
    "@babel/plugin-transform-optional-chaining": "^7.22.5",
    "@capacitor/android": "^5.7.0",
    "@capacitor/app": "^5.0.7",
    "@capacitor/assets": "^3.0.4",
    "@capacitor/cli": "^5.7.0",
    "@capacitor/core": "^5.7.0",
    "@capacitor/ios": "^5.7.0",
    "@types/hammerjs": "^2.0.41",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.16",
    "capacitor-set-version": "^2.2.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-vue": "9.19.2",
    "postcss": "^8.4.32",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.1",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "rxjs": "^7.8.0",
    "sass": "^1.54.9",
    "sass-loader": "13.3.2",
